{
  "name": "battery-technologies-node-vercel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --coverage --runInBand",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --reporter=line",
    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "migrate:analyze": "node scripts/migrate/analyze-source.js",
    "migrate:merge-deps": "node scripts/migrate/merge-package-json.js",
    "migrate:copy-code": "node scripts/migrate/copy-source-code.js",
    "migrate:from-legacy": "./scripts/migrate/migrate-from-legacy.sh",
    "setup:runtime": "node scripts/setup/fix-runtime-config.js",
    "setup:fix-refs": "node scripts/setup/fix-variable-refs.js",
    "setup:validate-deps": "node scripts/setup/validate-dependencies.js",
    "setup:validate-migration": "node scripts/setup/validate-migration.js",
    "dev:https": "node scripts/generate-https-certs.js && next dev --turbopack --experimental-https --experimental-https-key ./certificates/localhost-key.pem --experimental-https-cert ./certificates/localhost.pem",
    "dev:https:trusted": "node scripts/setup-trusted-certs.js && next dev --turbopack --experimental-https --experimental-https-key ./certificates/localhost-key.pem --experimental-https-cert ./certificates/localhost.pem",
    "generate-certs": "node scripts/generate-https-certs.js",
    "setup-trusted-certs": "node scripts/setup-trusted-certs.js",
    "db:migrate": "prisma migrate dev",
    "db:seed-real": "tsx prisma/seed-real-data.ts",
    "db:seed-simple": "tsx prisma/seed-simple.ts",
    "db:seed-comprehensive": "tsx prisma/seed-comprehensive.ts",
    "db:seed-scalability": "tsx prisma/seed-scalability.ts",
    "update-cache": "node scripts/update-cache-version.js",
    "generate-icons": "node scripts/generate-icons.js"
