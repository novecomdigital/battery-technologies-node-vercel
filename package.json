    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prisma": "^5.7.0",
    "tailwindcss": "^4.0.0-beta.7",
