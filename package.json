    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^6.0.2",
    "svix": "^1.76.1",
    "tailwind-merge": "^3.3.1",
    "workbox-background-sync": "^7.3.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
